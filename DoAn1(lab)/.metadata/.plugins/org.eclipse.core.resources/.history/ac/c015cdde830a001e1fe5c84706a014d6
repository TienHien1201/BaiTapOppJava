package quanLiNhanSu;

public class CanBoQuanLy extends ConNguoi{
private String namKinhNghiem;

public CanBoQuanLy(int maNhanVien, String hoVaTen, int namSinh, String goiTinh, String soDienThoai,
		String daThoiViec, float soGioLamViec, String namKinhNghiem) {
	super(maNhanVien, hoVaTen, namSinh, goiTinh, soDienThoai, daThoiViec, soGioLamViec);
	this.namKinhNghiem = namKinhNghiem;
}

public String getNamKinhNghiem() {
	return namKinhNghiem;
}

public void setNamKinhNghiem(String namKinhNghiem) {
	this.namKinhNghiem = namKinhNghiem;
}

@Override
public String toString() {
	return "|"+maNhanVien+"|" + hoVaTen +"|" + namSinh + "|" + goiTinh + "|"+ soDienThoai + "|" + namKinhNghiem + "|"+soGioLamViec+"|"+ daThoiViec+"|";
}

}
