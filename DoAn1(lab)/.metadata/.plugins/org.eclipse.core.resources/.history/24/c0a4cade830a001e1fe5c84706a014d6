package quanLiNhanSu;

public class NhanVienPartTime extends ConNguoi{
private String loaiCongViec;

public NhanVienPartTime(int maNhanVien, String hoVaTen, int namSinh, String goiTinh, String soDienThoai,
		String daThoiViec, float soGioLamViec, String loaiCongViec) {
	super(maNhanVien, hoVaTen, namSinh, goiTinh, soDienThoai, daThoiViec, soGioLamViec);
	this.loaiCongViec = loaiCongViec;
}

public String getLoaiCongViec() {
	return loaiCongViec;
}

public void setLoaiCongViec(String loaiCongViec) {
	this.loaiCongViec = loaiCongViec;
}

@Override
public String toString() {
	return "|"+maNhanVien+"|" + hoVaTen +"|" + namSinh + "|" + goiTinh + "|"+ soDienThoai + "|" + loaiCongViec + "|"+soGioLamViec+"|"+ daThoiViec+"|";
}

	
	
}
